@using MSDev.Task.Models
@{
	ViewBag.Title = "title";
	Layout = "_NewsLayout";
}

<table class="table table-hover table-striped">
	<thead>
		<tr>
			<th>标题</th>
			<th>来源</th>
			<th>时间</th>
			<th>操作</th>
		</tr>
	</thead>
	<tbody>
		@{
			foreach (BingNews news in (List<BingNews>) ViewBag.ListData)
			{
				<tr>
					<td>@news.Title</td>
					<td>@news.Provider</td>
					<td>@news.CreatedTime</td>
					<td>
						<button class="btn btn-sm btn-danger">删除</button>
					</td>
				</tr>
			}

			@ViewBag.Page
		}
	</tbody>
</table>